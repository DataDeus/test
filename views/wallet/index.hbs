				<script>
					(async function setAccountType() {
						const { value: accountType } = await Swal.fire({
							title: 'Select an Account Type',
							input: 'select',
							inputOptions: {
								'Client': 'Client',
								'Merchant': 'Merchant'
							},
							inputPlaceholder: 'Select an Account Type',
							allowOutsideClick: false,
							allowEscapeKey: false,
							showCloseButton: false,
							showCancelButton: false,
							showLoaderOnConfirm: true,
							inputValidator: (value) => {
								return new Promise((resolve) => {
									if (value != '') {
										resolve()
									} else {
										resolve('You need to select an Account type!')
									}
								})
							},
							preConfirm: (value) => {
								return new Promise((resolve) => {
									if (value == 'Merchant') {

										Swal.fire({
											title: 'Enter Merchant Name',
											focusConfirm: false,
											allowOutsideClick: false,
											allowEscapeKey: true,
											showCloseButton: true,
											input: 'text',
											showCancelButton: true,
											inputValidator: (value) => {
												if (!value) {
													return 'You need to write something!'
												}
											},
											showLoaderOnConfirm: true,
											preConfirm: (value) => {
												$.ajax({
													url: '/users/accType/update',
													type: 'POST',
													data: `acc_type='Merchant'&slug='${value}'`,
													dataType: 'json'
												})
													.then(response => {
														const Toast = Swal.mixin({
															toast: true,
															position: 'bottom',
															showConfirmButton: false,
															timer: 3000
														});

														Toast.fire({
															type: 'success',
															title: 'Account type updated'
														})
														//document.getElementById('merCode').innerHTML += 
													})
													.catch(error => {
														console.log(error)
													})
											}
										})
											.then(function (msg) {
												if (msg.dismiss) {
													setAccountType();
												}
											})

									} else {
										$.ajax({
											url: '/users/accType/update',
											type: 'POST',
											data: `acc_type=${value}&slug=client`,
											dataType: 'json'
										})
											.then(response => {
												const Toast = Swal.mixin({
													toast: true,
													position: 'bottom',
													showConfirmButton: false,
													timer: 3000
												});

												Toast.fire({
													type: 'success',
													title: 'Account type updated'
												})
												//document.getElementById('merCode').innerHTML += 
											})
											.catch(error => {
												console.log(error)
											})
									}
								})
							}
						})

					})();
				</script>
